<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.doing.canvas"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CanvasActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.TableCanvasActivity"/>
        <activity android:name=".activity.PixelColorCanvasActivity"/>
        <activity android:name=".surface.activity.SurfaceActivity"/>
        <activity android:name=".surface.activity.ColorsFrameActivity" />
        <activity android:name=".surface.activity.AlphaFrameActivity" />
    </application>

</manifest>